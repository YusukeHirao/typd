{
    "manifest_version": 2,
    "name": "typd (仮:フォームの入力値を自動で保存してCtrl+rで補完できるやつ)",
    "version": "0.3.1",
    "browser_action": {
        "default_icon": "off.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": [
            "js/jquery-2.1.3.min.js",
            "js/underscore-min.js",
            "js/aes.js",
            "js/sha256.js",
            "js/keymaster.js",
            "js/pubsub.js",
            "js/background.js"
        ]
    }, 
    "content_scripts": [
        {
            "matches": [ "http://*/*", "https://*/*" ],
            "js": [
                "js/jquery-2.1.3.min.js",
                "js/jquery.serializeObject.js",
                "js/underscore-min.js",
                "js/aes.js",
                "js/sha256.js",
                "js/keymaster.js",
                "js/pubsub.js",
                "js/inputlib.js",
                "js/script.js"
            ],
            "css": [
                "css/style.css"
            ]
        }
    ],
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    }
}
