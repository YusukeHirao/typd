{
    "manifest_version": 2,
    "name": "__MSG_extName__",
    "description": "__MSG_extDescription__",
    "version": "0.7.1",
    "default_locale": "ja",
    "icons": {
        "128": "icon_128.png",
        "48": "icon_48.png",
        "16": "icon_16.png"
    },
    "browser_action": {
        "default_icon": "on.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": [
            "js/jquery-2.1.3.min.js",
            "js/underscore-min.js",
            "js/aes.js",
            "js/sha256.js",
            "js/keymaster.js",
            "js/pubsub.js",
            "js/background.js"
        ]
    }, 
    "content_scripts": [
        {
            "matches": [ "http://*/*", "https://*/*" ],
            "js": [
                "js/jquery-2.1.3.min.js",
                "js/underscore-min.js",
                "js/aes.js",
                "js/sha256.js",
                "js/keymaster.js",
                "js/pubsub.js",
                "js/inputlib.js",
                "js/script.js"
            ],
            "css": [
                "css/style.css"
            ]
        }
    ],
    "options_page": "options.html"
}
